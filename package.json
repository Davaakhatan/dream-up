{
  "name": "dreamup-qa-agent",
  "version": "1.0.0",
  "description": "Autonomous AI agent for testing browser-based games",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "qa-agent": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "cli": "tsx src/cli.ts",
    "dashboard": "tsx src/dashboard/server.ts",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "qa",
    "automation",
    "browser-games",
    "ai-agent"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@browserbasehq/sdk": "^1.1.1",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "express": "^4.18.2",
    "openai": "^4.20.1",
    "ora": "^7.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.2"
  }
}

